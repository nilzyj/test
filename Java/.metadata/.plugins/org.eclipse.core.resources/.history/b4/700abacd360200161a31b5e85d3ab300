package leetcode;

public class test3 {

	public static void main(String[] args) 
	{
		String s="abcabcaa";
		System.out.println(lengthOfLongestSubstring(s));
	}
	
	public static int lengthOfLongestSubstring(String s) 
	{
		int maxlength=0,temp=0;
		for(int i;i<s.length;++i)
		{
			if(s[i+1]!=s[i])
			{
				temp++;
			}
		}
		return maxlength;
	}
}

